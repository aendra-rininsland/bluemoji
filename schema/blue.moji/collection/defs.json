{
  "lexicon": 1,
  "id": "blue.moji.collection.defs",
  "defs": {
    "itemView": {
      "type": "object",
      "required": ["name", "asset"],
      "properties": {
        "name": { "type": "string" },
        "alt": { "type": "string" },
        "createdAt": { "type": "string", "format": "datetime" },
        "asset": { "type": "bytes" },
        "adultOnly": { "type": "boolean", "default": false }
      }
    }
  }
}
